{
  "manifest_version": 2,
  "name": "ocr-extension",
  "version": "1.0",
  "description": "Recreates the webpage adding alt tag to images",
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "activeTab",
    "tabs"
  ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["main.js"]
  }]
}

{

  "manifest_version": 1,
  "name": "ocr-extension",
  "version": "1.0",
  "description": "Recreates the webpage adding alt tag to images",
  
  "permissions": [
    "<all_urls>",
    "contextMenus",
    "activeTab",
    "tabs",
    "*://developer.mozilla.org/*",
    "about:debugging"
  ],

  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["main.js"]
  }
  ]

}
